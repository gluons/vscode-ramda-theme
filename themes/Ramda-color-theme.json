{
	"name": "Ramda",
	"type": "dark",
	"colors": {
		"editor.background": "#282a36",
		"editor.foreground": "#f8f8f2",
		"editor.lineHighlightBackground": "#ffffff0D",
		"editor.selectionBackground": "#ffffff1a",
