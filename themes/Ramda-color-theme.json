	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeffffff",
				"background": "#263238ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6272a4"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"support.variable"
			],
			"settings": {
				"foreground": "#50fa7b"
			}
		},
		{
			"name": "Object Property",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key punctuation.definition.string",
				"meta.object-literal.key string.quoted",
				"variable.other.object.property",
				"variable.other.property",
				"support.variable.property"
			],
			"settings": {
				"foreground": "#66d9ef"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5F7C"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.type punctuation.definition.block",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#ff79c6"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#ff79c6"
			}
		},
		{
			"name": "Block Punctuation",
			"scope": [
				"punctuation.definition.block",
				"punctuation.definition.parameters"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ff79c6"
			}
		},
		{
			"name": "Function Declaration",
			"scope": [
				"meta.definition.function entity.name.function",
				"meta.function.expression variable.parameter"
			],
			"settings": {
				"foreground": "#ffb86c"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"meta.class meta.method.declaration storage.type"
			],
			"settings": {
				"foreground": "#66d9ef"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other",
				"meta.block support.variable"
			],
			"settings": {
				"foreground": "#50fa7b"
			}
		},
		{
			"name": "Variable Function",
			"scope": [
				"meta.definition.variable entity.name.function"
			],
			"settings": {
				"foreground": "#50fa7b"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#bd93f9"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
